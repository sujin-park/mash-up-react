{
  "compilerOptions": {
    "strict": true,
    "target": "ES5",
    "lib": [
      "es2019",
      "es2017",
      "es7",
      "es6",
      "dom",
      "dom.iterable"
    ],
    "jsx": "react",
    "noImplicitAny": false,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "moduleResolution": "node",
    "baseUrl": "./",
    "allowJs": true,
    "skipLibCheck": true,
    "allowSyntheticDefaultImports": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "module": "esnext",
    "isolatedModules": true,
    "noEmit": true
  },
  "exclude": [
    "node_modules"
  ],
  "include": [
    "src"
  ]
}
